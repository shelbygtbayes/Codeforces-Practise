Approach Max GCD of any 2 no's from [1...n] = floor( n / 2 );

#include<iostream>
#include <vector>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n , ans;
        cin >> n;
        if(n%2 == 0) ans = n/2;
        else ans = (n-1)/2; 
        cout << ans << endl;
    }
    
    return 0;
}
